<h2>Search results</h2>
<div>
  <div>
    <% offers.forEach((offer) => { %>
      <div class="border border-warning rounded dashboard-results">
        <div>
            <a href="./offer/<%= offer._id%>">
              <%= offer.location %> <br>
             Offered budget: <%= offer.budget %><br><br>
            </a>
          </div>
      </div>
    <% }) %>
  </div>
</div>